          <h5 class="card-title">My Result</h5>
       <table>
        <tr>
            <td><label>Roll No</label></td>
            <td>:- <%= one.rollno %></td>
        </tr>
        <tr>
            <td><label>Name</label></td>
            <td>:- <%= one.stdName %></td>
        </tr>

        <tr>
            <td><label>Date of Birth</label></td>
            <td>:- <%= one.dob %></td>
        </tr>
        <tr>
            <td><label>Score</label></td>
            <td>:- <%= one.score %></td>
        </tr>
    </table>
   </div>
  </div>
